<?php

namespace AppBundle\Entity;

/**
 * PagamentiDettagliEtgest
 */
class PagamentiDettagliEtgest
{
    /**
     * @var integer
     */
    private $idpag = '0';

    /**
     * @var string
     */
    private $idrif = '';

    /**
     * @var integer
     */
    private $iddettagli;


    /**
     * Set idpag
     *
     * @param integer $idpag
     *
     * @return PagamentiDettagliEtgest
     */
    public function setIdpag($idpag)
    {
        $this->idpag = $idpag;

        return $this;
    }

    /**
     * Get idpag
     *
     * @return integer
     */
    public function getIdpag()
    {
        return $this->idpag;
    }

    /**
     * Set idrif
     *
     * @param string $idrif
     *
     * @return PagamentiDettagliEtgest
     */
    public function setIdrif($idrif)
    {
        $this->idrif = $idrif;

        return $this;
    }

    /**
     * Get idrif
     *
     * @return string
     */
    public function getIdrif()
    {
        return $this->idrif;
    }

    /**
     * Get iddettagli
     *
     * @return integer
     */
    public function getIddettagli()
    {
        return $this->iddettagli;
    }
}

<?php

namespace AppBundle\Entity;

/**
 * BackofficePagamentiAgenzie
 */
class BackofficePagamentiAgenzie
{
    /**
     * @var string
     */
    private $cod = '';

    /**
     * @var string
     */
    private $descrizione = '';

    /**
     * @var string
     */
    private $agenzia = 'ETIta';

    /**
     * @var string
     */
    private $pagamentiSiap = '';

    /**
     * @var integer
     */
    private $idPagamento;


    /**
     * Set cod
     *
     * @param string $cod
     *
     * @return BackofficePagamentiAgenzie
     */
    public function setCod($cod)
    {
        $this->cod = $cod;

        return $this;
    }

    /**
     * Get cod
     *
     * @return string
     */
    public function getCod()
    {
        return $this->cod;
    }

    /**
     * Set descrizione
     *
     * @param string $descrizione
     *
     * @return BackofficePagamentiAgenzie
     */
    public function setDescrizione($descrizione)
    {
        $this->descrizione = $descrizione;

        return $this;
    }

    /**
     * Get descrizione
     *
     * @return string
     */
    public function getDescrizione()
    {
        return $this->descrizione;
    }

    /**
     * Set agenzia
     *
     * @param string $agenzia
     *
     * @return BackofficePagamentiAgenzie
     */
    public function setAgenzia($agenzia)
    {
        $this->agenzia = $agenzia;

        return $this;
    }

    /**
     * Get agenzia
     *
     * @return string
     */
    public function getAgenzia()
    {
        return $this->agenzia;
    }

    /**
     * Set pagamentiSiap
     *
     * @param string $pagamentiSiap
     *
     * @return BackofficePagamentiAgenzie
     */
    public function setPagamentiSiap($pagamentiSiap)
    {
        $this->pagamentiSiap = $pagamentiSiap;

        return $this;
    }

    /**
     * Get pagamentiSiap
     *
     * @return string
     */
    public function getPagamentiSiap()
    {
        return $this->pagamentiSiap;
    }

    /**
     * Get idPagamento
     *
     * @return integer
     */
    public function getIdPagamento()
    {
        return $this->idPagamento;
    }
}

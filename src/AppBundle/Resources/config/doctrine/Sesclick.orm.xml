<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Sesclick" table="sesclick">
    <indexes>
      <index name="adv" columns="adv"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="adv" type="string" column="adv" length="1" nullable="false">
      <options>
        <option name="fixed">1</option>
        <option name="default"/>
      </options>
    </field>
    <field name="language" type="string" column="language" length="1" nullable="false">
      <options>
        <option name="fixed">1</option>
        <option name="default"/>
      </options>
    </field>
    <field name="data" type="date" column="data" nullable="false">
      <options>
        <option name="default">0000-00-00</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>

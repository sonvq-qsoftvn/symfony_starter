<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\BackofficeAgencyfee" table="backoffice_agencyfee">
    <id name="id" type="smallint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="descrizione" type="string" column="descrizione" length="100" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="importo" type="float" column="importo" precision="10" scale="0" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="iva" type="boolean" column="iva" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="tipo" type="string" column="tipo" length="10" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>

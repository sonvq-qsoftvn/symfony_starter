<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\CorsiTipo" table="corsi_tipo">
    <id name="idcorsitipo" type="smallint" column="idCorsiTipo">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="descrizionetipo" type="string" column="descrizioneTipo" length="120" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="restrizioni" type="string" column="restrizioni" length="100" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="gruppo" type="string" column="gruppo" length="50" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="evidenza" type="boolean" column="evidenza" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="ripetibile" type="boolean" column="ripetibile" nullable="false">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="notificatipo" type="string" column="notificaTipo" length="10" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="descrizioneestesa" type="string" column="descrizioneEstesa" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="punteggio" type="smallint" column="punteggio" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>

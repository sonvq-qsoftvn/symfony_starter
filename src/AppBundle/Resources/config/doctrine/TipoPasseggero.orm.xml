<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\TipoPasseggero" table="tipo_passeggero">
    <id name="idtipopasseggero" type="integer" column="idTipoPasseggero">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="descrizione" type="string" column="descrizione" length="100" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="datacreazione" type="datetime" column="datacreazione" nullable="true"/>
    <field name="datamodifica" type="datetime" column="datamodifica" nullable="true"/>
  </entity>
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ET\ETGestBundle\Entity\Allegati" table="allegati" repository-class="ET\ETGestBundle\Repository\AllegatiRepository">
    <indexes>
      <index name="id_modulo" columns="id_modulo,tipoDocumento"/>
    </indexes>
    <id name="idallegato" type="integer" column="idAllegato">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="data" type="datetime" column="data" nullable="false">
      <options>
        <option name="default">0000-00-00 00:00:00</option>
      </options>
    </field>
    <field name="percorso" type="string" column="percorso" length="100" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="nomefile" type="string" column="nomeFile" length="100" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="tipodocumento" type="integer" column="tipoDocumento" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="idModulo" type="integer" column="id_modulo" nullable="false">
      <options>
        <option name="unsigned">1</option>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>

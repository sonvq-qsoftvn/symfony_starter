<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ET\ETGestBundle\Entity\PagamentiMetodi" table="pagamenti_metodi">
    <id name="codice" type="string" column="codice" length="20">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="titolo" type="string" column="titolo" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descrizione" type="string" column="descrizione" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="agenzia" type="string" column="agenzia" length="10" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>

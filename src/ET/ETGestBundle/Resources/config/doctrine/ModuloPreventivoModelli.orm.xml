        <option name="fixed"/>
      </options>
    </field>
    <field name="predefinito" type="boolean" column="predefinito" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="pubblico" type="boolean" column="pubblico" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>

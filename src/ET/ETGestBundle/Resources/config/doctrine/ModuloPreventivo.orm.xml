        <option name="fixed"/>
      </options>
    </field>
    <field name="conclusioni" type="blob" column="conclusioni" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="statusaves" type="boolean" column="statusAves" nullable="false">

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ET\ETGestBundle\Entity\ServiziIncasso" table="servizi_incasso">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idTipologia" type="bigint" column="id_tipologia" nullable="true">
      <options>
        <option name="unsigned"/>
        <option name="default">0</option>
      </options>
    </field>
    <field name="incassato" type="decimal" column="incassato" precision="7" scale="2" nullable="true">
      <options>
        <option name="default">0.00</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>

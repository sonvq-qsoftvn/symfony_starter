<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ET\ETTicketBundle\Entity\RatingProducts" table="rating_products">
    <id name="ratingUserId" type="integer" column="rating_user_id"/>
    <id name="portale" type="string" column="portale" length="100"/>
    <id name="lingua" type="string" column="lingua" length="10"/>
    <field name="ratingR1" type="float" column="rating_r1" precision="8" scale="5" nullable="false">
      <options>
        <option name="default">0.00000</option>
      </options>
    </field>
    <field name="ratingR2" type="float" column="rating_r2" precision="8" scale="5" nullable="false">
      <options>
        <option name="default">0.00000</option>
      </options>
    </field>
    <field name="ratingR3" type="float" column="rating_r3" precision="8" scale="5" nullable="false">
      <options>
        <option name="default">0.00000</option>
      </options>
    </field>
    <field name="ratingR" type="float" column="rating_r" precision="8" scale="5" nullable="false">
      <options>
        <option name="default">0.00000</option>
      </options>
    </field>
    <field name="ratingDate" type="datetime" column="rating_date" nullable="false">
      <options>
        <option name="default">0000-00-00 00:00:00</option>
      </options>
    </field>
    <field name="nAssignedRequests" type="integer" column="n_assigned_requests" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nAcceptedRequests" type="integer" column="n_accepted_requests" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nWorkedRequests" type="integer" column="n_worked_requests" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="nClosedRequests" type="integer" column="n_closed_requests" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="isBestrate" type="boolean" column="is_bestrate" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>

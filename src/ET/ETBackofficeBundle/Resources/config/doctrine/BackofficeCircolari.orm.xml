<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ET\ETBackofficeBundle\Entity\BackofficeCircolari" table="backoffice_circolari">
    <id name="idcircolare" type="integer" column="IDCircolare">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="titolocircolare" type="string" column="TitoloCircolare" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="titolarecircolare" type="string" column="TitolareCircolare" length="100" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="datacircolare" type="datetime" column="DataCircolare" nullable="false">
      <options>
        <option name="default">0000-00-00 00:00:00</option>
      </options>
    </field>
    <field name="numerocircolare" type="string" column="NumeroCircolare" length="50" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="filecircolare" type="string" column="FileCircolare" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
